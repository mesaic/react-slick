{
  "name": "@wish.technology/react-slick",
  "description": "Fork of react-slick",
  "main": "./dist",
  "files": [
    "dist",
    "slick.css"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel -d dist src",
    "lint": "eslint src",
    "test": "mocha src/**/*.test.js",
    "tdd": "npm run test -- -w",
    "validate": "npm-run-all --parallel lint build test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "wish.technology",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wish-technology/react-slick"
  },
  "bugs": {
    "url": "https://github.com/wish-technology/react-slick/issues"
  },
  "homepage": "https://github.com/wish-technology/react-slick",
  "keywords": [
    "slick",
    "carousel",
    "Image slider",
    "orbit",
    "slider",
    "react-component"
  ],
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "6.0.2",
    "babel-plugin-espower": "2.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "cz-conventional-changelog": "1.1.5",
    "deepmerge": "^0.2.10",
    "del": "^2.2.0",
    "enzyme": "2.2.0",
    "es5-shim": "^4.1.7",
    "eslint": "^2.6.0",
    "eslint-config-wish-technology": "2.0.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-flow-vars": "0.3.0",
    "eslint-plugin-react": "4.3.0",
    "ghooks": "1.2.0",
    "jsdom": "8.3.1",
    "mocha": "2.4.5",
    "npm-run-all": "1.7.0",
    "power-assert": "1.3.1",
    "prettify-xml": "1.0.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "^15.0.0",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "2.5.0",
    "with-package": "0.2.0"
  },
  "dependencies": {
    "can-use-dom": "0.1.0",
    "classnames": "^2.1.1",
    "enquire.js": "2.1.1",
    "invariant": "2.2.1",
    "json2mq": "^0.2.0",
    "object-assign": "^4.0.1",
    "react-responsive-mixin": "^0.3.5"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "wish-technology"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run validate -s"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
